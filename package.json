{
  "name": "@chunkangwong/grid-query-features",
  "description": "A React hook library for querying ArcGIS service features from MUI DataGrid v6 with server mode pagination, filtering, and sorting.",
  "author": {
    "name": "Chun Kang WONG",
    "email": "chunkang.wong94@gmail.com"
  },
  "keywords": [
    "arcgis",
    "react",
    "mui",
    "datagrid"
  ],
  "version": "0.0.12",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkangwong/grid-query-features"
  },
  "main": "./dist/useGridQueryFeatures.umd.js",
  "types": "./dist/useGridQueryFeatures.d.ts",
  "module": "./dist/useGridQueryFeatures.es.js",
  "exports": {
    ".": {
      "types": "./dist/useGridQueryFeatures.d.ts",
      "import": "./dist/useGridQueryFeatures.es.js",
      "require": "./dist/useGridQueryFeatures.umd.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build"
  },
  "dependencies": {
    "@arcgis/core": "^4.27.6",
    "@mui/x-data-grid": "^6.16.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  }
}
